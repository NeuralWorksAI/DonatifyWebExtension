{
    "manifest_version": 2,
    "name": "Donatify",
    "description":"Round up your purchase total on ecommerce sites and donate a bit each time to the charities you resonate with most. A little bit goes a long way!",
    "version":"1.0",
    "icons":{
        "16":"/images/donatify-favicon.png",
        "48":"/images/donatify-favicon.png",
        "128":"/images/donatify-favicon.png"
    },
    "browser_action":{
        "default_popup":"login.html",
        "default_icon":{
                    "16":"/images/donatify-favicon.png",
                    "48":"/images/donatify-favicon.png",
                    "128":"/images/donatify-favicon.png"
                }
    },
    "permissions": [ 
        "tabs"
    ]
     
  
}